Dispel)
/script dispel_prio()

LUA)
function dispel()
for x=1,16 do
local name,count,debuffType=UnitDebuff("target",x,1)
if (debuffType=="Magic")
then
cast("Dispel Magic")
end
end
end

function heal_after_dispel()
heal_raid()
end

function dispel_prio()
TargetByName("Llanewrynn",true)
dispel()
TargetByName("Yxon",true)
dispel()
TargetByName("Skyx",true)
dispel()
TargetByName("Uyalin",true)
dispel()
TargetByName("Paladino",true)
dispel()
dispel_raid()
heal_after_dispel()
end

function dispel_raid()
TargetUnit("raid1")
dispel()
TargetUnit("raid2")
dispel()
TargetUnit("raid3")
dispel()
TargetUnit("raid4")
dispel()
TargetUnit("raid5")
dispel()
TargetUnit("raid6")
dispel()
TargetUnit("raid7")
dispel()
TargetUnit("raid8")
dispel()
TargetUnit("raid9")
dispel()
TargetUnit("raid10")
dispel()
TargetUnit("raid11")
dispel()
TargetUnit("raid12")
dispel()
TargetUnit("raid13")
dispel()
TargetUnit("raid14")
dispel()
TargetUnit("raid15")
dispel()
TargetUnit("raid16")
dispel()
TargetUnit("raid17")
dispel()
TargetUnit("raid18")
dispel()
TargetUnit("raid19")
dispel()
TargetUnit("raid20")
dispel()
end
