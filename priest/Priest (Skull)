Priest (Skull))
/script dps_skull()

LUA)
function dps_skull()
if (GetRaidTargetIndex("target")==8)
then
attack()
else
target_skull()
end
end

function attack()
if (UnitMana("player")>=376)
then
PickupAction(63)
PlaceAction(62)
shadow_priest()
else
if not IsCurrentAction(62)
then
UseAction(62)
PickupAction(62)
PlaceAction(63)
end
end
end

function shadow_priest()
shadow_pain()
cast("Mind Blast")
end

function shadow_pain()
local i,x=1,0
while (UnitDebuff("target",i))
do if (UnitDebuff("target",i)=="Interface\\Icons\\Spell_Shadow_ShadowWordPain")
then x=1
end
i=i+1
end
if (x==0)
then
cast("Shadow Word: Pain")
end
end

function target_skull()
TargetByName("Gaelber")
TargetUnit("targettarget")
if (GetRaidTargetIndex("target")==8)
then
SendChatMessage("Targeted %t!")
return
else
TargetByName("Llanewrynn")
TargetUnit("targettarget")
end
if (GetRaidTargetIndex("target")==8)
then
SendChatMessage("Targeted %t!")
return
else
TargetByName("Yxon")
TargetUnit("targettarget")
end
if (GetRaidTargetIndex("target")==8)
then
SendChatMessage("Targeted %t!")
return
else
tab_target_skull()
end
end

function tab_target_skull()
for i=1,10
do TargetNearestEnemy()
if (GetRaidTargetIndex("target")==8)
then
SendChatMessage("Targeted %t!")
return
else
TargetUnit("player")
end
end
end
