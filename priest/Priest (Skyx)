Priest)
/script heal_raid()

LUA)
function heal_raid()
heal_tanks()
heal_healers()
heal_dps()
end

function heal_tanks()
TargetByName("Llanewrynn",true)
h_40_t()
h_60_t()
h_70_t()
TargetByName("Yxon",true)
h_40_t()
h_60_t()
h_70_t()
TargetByName("Gaelber",true)
h_40_t()
h_60_t()
h_70_t()
end

function heal_healers()
TargetByName("Skyx",true)
fade()
h_70_h()
TargetByName("Uyalin",true)
h_50_h()
h_70_h()
TargetByName("Dreamblast",true)
h_50_h()
h_70_h()
TargetByName("Illumyn",true)
h_50_h()
h_70_h()
TargetByName("Paladino",true)
h_50_h()
h_70_h()
TargetByName("Kearlah",true)
h_50_h()
h_70_h()
end

function heal_dps()
TargetByName("Reimus",true)
h_50_d()
h_70_d()
TargetByName("Necropoly",true)
h_50_d()
h_70_d()
TargetByName("Vibranium",true)
h_50_d()
h_70_d()
TargetByName("Dorkilla",true)
h_50_d()
h_70_d()
TargetByName("Tygerra",true)
h_50_d()
h_70_d()
TargetByName("Dorothy",true)
h_50_d()
h_70_d()
TargetByName("Skyfire",true)
h_50_d()
h_70_d()
TargetByName("Eoro",true)
h_50_d()
h_70_d()
TargetByName("Livingbomb",true)
h_50_d()
h_70_d()
TargetByName("Havox",true)
h_50_d()
h_70_d()
TargetByName("Lazarrus",true)
h_50_d()
h_70_d()
end
