Paladin Buffs)
/target Gaelber
/script kings()
/target Kearlah
/script kings()
/target Dreamblast
/script kings()
/target Dorkilla
/script b_of_kings()
/target Havox
/script kings()
/target Dorothy
/script kings()
/target Skyfire
/script kings()

LUA)
function kings()
local i,x=1,0
while (UnitBuff("target",i))
do if (UnitBuff("target",i)=="Interface\\Icons\\Spell_Magic_GreaterBlessingofKings")
then x=1
end
i=i+1
end
if (x==0)
then cast("Greater Blessing of Kings")
end
end
