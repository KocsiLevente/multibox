Cleanse)
/target Llanewrynn
/script cleanse()
/target Paladino
/script cleanse()
/target Skyx
/script cleanse()
/target Livingbomb
/script cleanse()
/target Reimus
/script cleanse()

LUA)
function cleanse()
for x=1,16 do
local name,count,debuffType=UnitDebuff("target",x,1)
if (debuffType=="Magic" or debuffType=="Disease" or debuffType=="Poison")
then
cast("Cleanse")
end
end
end
