function h_85_t()
if (UnitHealth("target")/UnitHealthMax("target")>0.0001)
then
if (UnitHealth("target")/UnitHealthMax("target")<0.85)
then
CastSpellByName("Flash of Light(Rank 1)")
end
end
end
