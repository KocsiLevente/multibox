Firelord)
/script firelord()

LUA)
function firelord()
dps_spawn()
dps_firelord()
end

function dps_spawn()
if (UnitName("target")==("Lava Spawn"))
then
attack()
return
end
TargetByName("Gaelber")
TargetUnit("targettarget")
if (UnitName("target")==("Lava Spawn"))
then
attack()
return
end
TargetByName("Llanewrynn")
TargetUnit("targettarget")
if (UnitName("target")==("Lava Spawn"))
then
attack()
return
end
TargetByName("Yxon")
TargetUnit("targettarget")
if (UnitName("target")==("Lava Spawn"))
then
attack()
return
end
end

function dps_firelord()
if (UnitName("target")==("Firelord"))
then
attack()
return
end
TargetByName("Gaelber")
TargetUnit("targettarget")
if (UnitName("target")==("Firelord"))
then
attack()
return
end
TargetByName("Llanewrynn")
TargetUnit("targettarget")
if (UnitName("target")==("Firelord"))
then
attack()
return
end
TargetByName("Yxon")
TargetUnit("targettarget")
if (UnitName("target")==("Firelord"))
then
attack()
return
end
end
