Decurse)
/target Llanewrynn
/script decurse()
/target Skyx
/script decurse()
/target Livingbomb
/script decurse()
/target Reimus
/script decurse()
/target Tygerra
/script decurse()

LUA)
function decurse()
for x=1,16 do
local name,count,debuffType=UnitDebuff("target",x,1)
if debuffType=="Curse"
then
cast("Remove Lesser Curse")
end
end
end
