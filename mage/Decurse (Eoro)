Decurse)
/target Paladino
/script decurse()
/target Uyalin
/script decurse()
/target Eoro
/script decurse()
/target Necropoly
/script decurse()
/target Vibranium
/script decurse()

LUA)
function decurse()
for x=1,16 do
local name,count,debuffType=UnitDebuff("target",x,1)
if debuffType=="Curse"
then
cast("Remove Lesser Curse")
end
end
end
